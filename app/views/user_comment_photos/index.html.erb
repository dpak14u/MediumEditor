<p id="notice"><%= notice %></p>

<h1>Listing User Comment Photos</h1>

<table>
  <thead>
    <tr>
      <th>Photo</th>
      <th>Body</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_comment_photos.each do |user_comment_photo| %>
      <tr>
        <td><%= user_comment_photo.photo %></td>
        <td><%= user_comment_photo.body %></td>
        <td><%= user_comment_photo.user_id %></td>
        <td><%= link_to 'Show', user_comment_photo %></td>
        <td><%= link_to 'Edit', edit_user_comment_photo_path(user_comment_photo) %></td>
        <td><%= link_to 'Destroy', user_comment_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User comment photo', new_user_comment_photo_path %>
